import pandas as pd  

# Creating a dictionary with month names as index and numbers as values
months = {
    "January": 1, "February": 2, "March": 3, "April": 4,
    "May": 5, "June": 6, "July": 7, "August": 8,
    "September": 9, "October": 10, "November": 11, "December": 12
}

# Creating a Series
month_series = pd.Series(months)  

# Printing the Series
print(month_series)  
